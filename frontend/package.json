{
  "name": "alum-net",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn install",
    "prebuild:android": "cd apps/mobile && npx expo prebuild --clean --platform android",
    "run:web": "cd apps/web && expo start --web",
    "run:android": "cd apps/mobile && expo run:android",
    "build:web": "cd apps/web && expo build:web",
    "build:android": "cd apps/mobile && eas build --local --platform android",
    "lint": "eslint . --fix",
    "tsc": "tsc",
    "prettier:fix": "prettier --write ./src",
    "prettier": "prettier --check ./src"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.12.2",
    "expo": "53.0.22",
    "expo-constants": "~17.1.7",
    "expo-device": "~7.1.4",
    "expo-font": "^11.10.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.7",
    "expo-system-ui": "^2.9.3",
    "expo-web-browser": "^15.0.8",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-mmkv": "^3.3.3",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.38.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.2.1",
    "jest-expo": "^53.0.10",
    "prettier": "^3.6.2",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.46.1"
  },
  "resolutions": {
    "@expo/config-plugins": "~10.1.1"
  }
}
