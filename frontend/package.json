{
  "name": "alum-net",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn install",
    "run:web": "cd apps/web && expo start --web",
    "run:android": "cd apps/mobile && expo run:android",
    "build:web": "cd apps/web && expo build:web",
    "build:android": "cd apps/mobile && eas build --local --platform android"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "53.0.22",
    "expo-font": "^11.10.2",
    "expo-linking": "^6.2.2",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "^0.26.4",
    "expo-system-ui": "^2.9.3",
    "expo-web-browser": "^15.0.8",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-mmkv": "^3.3.3",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.2.1",
    "jest-expo": "^53.0.10",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.3.3"
  },
  "resolutions": {
    "@expo/config-plugins": "~10.1.1"
  }
}