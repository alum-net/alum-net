{
  "expo": {
    "name": "alumnet",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../../packages/ui/src/assets/alumnet_logo.png",
    "scheme": "alumnet.mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "../../packages/ui/src/assets/alumnet_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "alumnet.mobile",
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "../../packages/ui/src/assets/alumnet_logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-document-picker",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}